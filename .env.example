# .env.example - 请复制为 .env 并填写真实值（不要把真实凭据提交到仓库）
# 用法：Docker Compose 会读取此文件中的变量。示例中的默认值仅供本地开发参考。

PROJECT_NAME=ai-agent-platform

# mysql service
MYSQL_DATABASE_HOST=mysql
MYSQL_ROOT_PASSWORD=chenhao20030210
MYSQL_DATABASE=ai_agent_db
MYSQL_USER=test_user
MYSQL_PASSWORD=12345678
MYSQL_PORT=3306

# phpmyadmin service
PMA_PORT=8081


# redis service
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=12345678

# backend service
BACKEND_PORT=8080
SPRING_PROFILES_ACTIVE=prod
SPRING_DATASOURCE_URL=jdbc:mysql://mysql:3306/${MYSQL_DATABASE}?useSSL=false&serverTimezone=Asia/Shanghai&allowPublicKeyRetrieval=true&characterEncoding=UTF-8
SPRING_DATASOURCE_USERNAME=${MYSQL_USER}
SPRING_DATASOURCE_PASSWORD=${MYSQL_PASSWORD}
SPRING_REDIS_HOST=${REDIS_HOST}
SPRING_REDIS_PORT=${REDIS_PORT}
SPRING_REDIS_PASSWORD=${REDIS_PASSWORD}
JAVA_OPTS=-Xms512m -Xmx1g
TZ=Asia/Shanghai

JWT_SECRET=change_me_jwt_secret

# frontend service
VITE_API_BASE_URL=http://backend:8080



# plugin-server service
PLUGIN_SERVER_HOST=0.0.0.0
PLUGIN_SERVER_PORT=3000

# rag service
RAG_MODEL_NAME=text-embedding-v1
RAG_ON_DOCKER=Y
DASHSCOPE_API_KEY=
QDRANT_PORT=6333
QDRANT_GRPC_PORT=6334
QDRANT_HOST=qdrant
