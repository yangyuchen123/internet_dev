{
  "defaultProvider": "deepseek",
  "providers": {
    "gpt": {
      "apiKeyEnv": "OPENAI_API_KEY",
      "baseURL": "https://api.openai.com/v1",
      "defaultModel": "gpt-4o-mini",
      "availableModels": [
        "gpt-4o-mini",
        "gpt-4o"
      ]
    },
    "deepseek": {
      "apiKeyEnv": "DEEPSEEK_API_KEY",
      "baseURL": "https://api.deepseek.com/v1",
      "defaultModel": "deepseek-chat",
      "availableModels": [
        "deepseek-chat"
      ]
    }
  }
}
